{
  "name": "sanity-plugin-media-video",
  "version": "2.0.0",
  "description": "A Sanity plugin for adding a media object (Image/Video) to your sanity studio schemas and displaying the media with built-in functionalities such as auto-play, custom PiP on scroll, etc.",
  "keywords": [
    "sanity",
    "sanity-plugin",
    "sanity-plugin-media-video"
  ],
  "homepage": "https://github.com/evelan-de/sanity-plugin-media-video#readme",
  "bugs": {
    "url": "https://github.com/evelan-de/sanity-plugin-media-video/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/evelan-de/sanity-plugin-media-video.git"
  },
  "license": "MIT",
  "author": "Evelan <kontakt@evelan.de>",
  "sideEffects": false,
  "type": "commonjs",
  "style": "./dist/sanity-plugin-media-video.css",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./types": {
      "source": "./src/exports/types/index.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.js",
      "default": "./dist/types.js"
    },
    "./renderer": {
      "source": "./src/exports/components/index.ts",
      "import": "./dist/renderer.mjs",
      "require": "./dist/renderer.js",
      "default": "./dist/renderer.js"
    },
    "./hooks": {
      "source": "./src/exports/hooks/index.ts",
      "import": "./dist/hooks.mjs",
      "require": "./dist/hooks.js",
      "default": "./dist/hooks.js"
    },
    "./contexts": {
      "source": "./src/exports/contexts/index.ts",
      "import": "./dist/contexts.mjs",
      "require": "./dist/contexts.js",
      "default": "./dist/contexts.js"
    },
    "./dist/sanity-plugin-media-video.css": "./dist/sanity-plugin-media-video.css",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "types": [
        "./dist/types.d.ts"
      ],
      "hooks": [
        "./dist/hooks.d.ts"
      ],
      "contexts": [
        "./dist/contexts.d.ts"
      ],
      "renderer": [
        "./dist/renderer.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "dist/*.css",
    "types.js",
    "renderer.js",
    "hooks.js",
    "contexts.js",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build-css": "sass src/css/styles.scss:dist/sanity-plugin-media-video.css --style compressed",
    "build": "npm run build-css && plugin-kit verify-package --silent && pkg-utils build --strict --clean && npm run build-css && pkg-utils check",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "npm run build-css && plugin-kit link-watch",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "prepublishOnly": "npm run build",
    "watch": "npm run build-css && pkg-utils watch --strict",
    "prepare": "husky"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "1.1.15",
    "@sanity/incompatible-plugin": "1.0.5",
    "@sanity/ui": "3.1.11",
    "react-intersection-observer": "9.16.0",
    "react-player": "2.16.0",
    "sanity-image": "1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@sanity/pkg-utils": "10.2.5",
    "@sanity/plugin-kit": "4.0.20",
    "@sanity/semantic-release-preset": "6.0.0",
    "@types/react": "19.2.7",
    "@typescript-eslint/eslint-plugin": "8.40.0",
    "@typescript-eslint/parser": "8.40.0",
    "autoprefixer": "10.4.21",
    "clsx": "2.1.1",
    "eslint": "8.57.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-config-sanity": "7.1.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unused-imports": "4.2.0",
    "husky": "9.1.7",
    "lint-staged": "16.1.5",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "prettier-plugin-packagejson": "2.5.19",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "sanity": "5.1.0",
    "sass": "1.90.0",
    "styled-components": "6.1.19",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "react": "^19",
    "sanity": "^5",
    "sanity-plugin-mux-input": "^2.8.1",
    "zod": "^4"
  },
  "engines": {
    "node": ">=18"
  },
  "overrides": {
    "sanity-plugin-mux-input": {
      "sanity": "$sanity"
    },
    "conventional-changelog-conventionalcommits": ">= 8.0.0"
  },
  "sanityPlugin": {
    "linkWatch": {
      "command": "npm run build-css && npm run watch",
      "extensions": "js,ts,tsx,png,svg,gif,jpeg,css,scss"
    },
    "verifyPackage": {
      "scripts": false
    }
  }
}
